- title t('login')

- form_for @user_session do |f|
    = f.error_messages
    %p
        = f.label t('username')
        %br/
        = f.text_field :username
    %p
        = f.label t('password')
        %br/
        = f.password_field :password
    %p
        = f.submit t('login')

.hint
    =t('js_needed')
